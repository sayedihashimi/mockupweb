@page "{MockupPath?}"
@model MockupModel
@{ 
    var mockupPathUrlEncoded = System.Net.WebUtility.UrlEncode(Model.MockupPath);
}
<ul id="mockupPageList">
    @foreach (var mpage in Model.MockupPages){
        <li>
           
            <a href="/ViewMockupPage?MockupPath=@mockupPathUrlEncoded&mockupName=@System.Net.WebUtility.UrlEncode(mpage.name)">@mpage.name</a>
        </li>
    }
</ul>

<script>
    window.onload = function () {
        PreloadImages('@Model.ImageUrlsString');
    };
</script>

